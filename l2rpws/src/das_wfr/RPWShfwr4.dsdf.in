description	= 'Cassini RPWS High Frequency Waveform Receiver calibrated spectra'
form		= x_tagged_y_scan ; time tag and frequency spectrum
reader		= '%(INST_ETC)s/invoke.sh rpws_l2wfr_hirdr 4'
spacecraft	= 'Cassini'
spacecraft_id	= 'CA'
instrument	= 'Radio Plasma Wave Subsystem'
instrument1	= 'High Frequency Waveform Receiver Channel 4'
instrument_id	= 'RPWS'
instrument_id1  = 'HFWR'
x_parameter	= 'time'
x_unit		= 's'
x_sample_width	= 600.00 ; typical seconds between spectra
y_parameter	= 'frequency'
y_unit		= 'KHz'
y_coordinate	= findgen(1025) / (2048.0 * 140.e-3)
z_parameter	= 'amplitude'
z_unit		= '(V/M)^2/Hz'
z_fill		= 1.0e-32 ; substitute this in place of missing data
